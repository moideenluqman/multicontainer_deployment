{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"moideenluqman/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"server",
            "image":"moideenluqman/multi-server",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"moideenluqman/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"moideenluqman/multi-nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80

                }
            ],
            "links":[ "client","server"]
         
        }

    ]
}